{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 640,
    "height": 480,
    "title": "Tuberculosis Incidence Over Time by Continent",
    "data": {
      "url": "https://raw.githubusercontent.com/leafianna/fit3179/main/Assign2/Data/incidence_con.csv",
      "format": {
        "type": "csv"
      }
    },
    "mark": "line",
    "encoding": {
      "x": {"field": "Year", "type": "quantitative", "title": "Year"},
      "y": {"field": "Incidence_count_100k","aggregate": "sum", "type": "quantitative", "title": "Incidence per 100,000 people"}
    },
    "config": {
      "axis": {
        "grid": false
      }
    }
  }